// room script file

function room_LeaveLeft()
{
    player.ChangeRoom(309, 305, 65, eDirectionLeft);
}

function room_LeaveRight()
{
    if (player.y < 100) {
        player.ChangeRoom(305, 15, 85, eDirectionRight);
    }
    else {
        player.ChangeRoom(305, 15, 180, eDirectionRight);
    }
}
