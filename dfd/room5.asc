// room script file

function room_LeaveTop()
{
    if (player.x > 250) {
        player.ChangeRoom(4, 60,  180, eDirectionRight);
    }
}

function room_LeaveRight()
{
    player.ChangeRoom(6, 15,  150, eDirectionRight);
}

function room_LeaveLeft()
{
    if (player.y < 100) {
        player.ChangeRoom(10, 308,  77, eDirectionLeft);
    }
    else {
        player.ChangeRoom(10, 305,  178, eDirectionLeft);
    }
}
