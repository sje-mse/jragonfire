// room script file

function room_LeaveLeft()
{
    player.ChangeRoom(9, 305, 65, eDirectionLeft);
}

function room_LeaveRight()
{
    if (player.y < 100) {
        player.ChangeRoom(5, 15, 85, eDirectionRight);
    }
    else {
        player.ChangeRoom(5, 15, 180, eDirectionRight);
    }
}

function room_LeaveTop()
{
    if (player.y > 100 && player.x < 40) {
        player.ChangeRoom(11, 115, 100,  eDirectionDown);
    }
}
