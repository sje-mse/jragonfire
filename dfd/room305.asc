// room script file

function room_LeaveTop()
{
    if (player.x > 250) {
        player.ChangeRoom(304, 60,  180, eDirectionRight);
    }
}

function room_LeaveRight()
{
    player.ChangeRoom(306, 15,  150, eDirectionRight);
}

function room_LeaveLeft()
{
    if (player.y < 100) {
        player.ChangeRoom(310, 308,  77, eDirectionLeft);
    }
    else {
        player.ChangeRoom(310, 305,  178, eDirectionLeft);
    }
}
